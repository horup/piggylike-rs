pub const TILES = 0;
pub const THINGS = 1;

pub fn define(engine) {
    engine.define_atlas(TILES, #{
        columns:16,
        rows:16,
        width:16.0,
        height:16.0,
        texture_path:"textures/tiles.png"
    });

    engine.define_atlas(THINGS, #{
        columns:16,
        rows:16,
        width:16.0,
        height:16.0,
        texture_path:"textures/things.png"
    });
    //define_atlas(TILES, 1);
}